{
  "module": {
    "name": "wifi_iot",
    "type": "har",
    "deviceTypes": [
      "default"
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.SET_WIFI_INFO",
        "reason": "$string:set_wifi",
      },
      {
        "name": "ohos.permission.GET_WIFI_INFO",
        "reason": "$string:get_wifi"
      },
      {
        "name": "ohos.permission.LOCATION",
        "reason": "$string:get_location"
      },
      {
        "name": "ohos.permission.APPROXIMATELY_LOCATION",
        "reason": "$string:get_approximately_location"
      }
    ]
  }
}
